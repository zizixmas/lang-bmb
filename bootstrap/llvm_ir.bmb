    t1 + t2 + t3 + t4 + t5;

fn test_const_generation() -> i64 =
    let c1 = gen_const_int("%_t0", 42);
    let t1 = if c1 == "%_t0 = add i64 0, 42" then 1 else 0;
    let c2 = gen_const_bool("%_t1", 1);
    let t2 = if c2 == "%_t1 = add i1 0, 1" then 1 else 0;
