// Fibonacci calculation for bootstrap compiler validation
// This file uses only features supported by the bootstrap compiler:
// - Integer arithmetic
// - Function calls
// - If-then-else
// - Recursion

// Recursive fibonacci
fn fib(n: i64) -> i64 =
    if n <= 1 { n } else { fib(n - 1) + fib(n - 2) };

// Main function that calculates fib(10) = 55
fn main() -> i64 =
    let result = fib(10);
    let u0 = println(result);
    0;
